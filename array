#include <stdio.h>

int main() {
    // Declare an array of integers
    int numbers[5];

    // Initialize the array elementsw
    numbers[0] = 10;
    numbers[1] = 20;
    numbers[2] = 30;
    numbers[3] = 40;
    numbers[4] = 50;

    // Access and print array elements
    printf("Element at index 0: %d\n", numbers[0]);
    printf("Element at index 1: %d\n", numbers[1]);
    printf("Element at index 2: %d\n", numbers[2]);
    printf("Element at index 3: %d\n", numbers[3]);
    printf("Element at index 4: %d\n", numbers[4]);

    // Calculate and print the sum of array elements
    int sum = 0;
    for (int i = 0; i < 5; i++) {
        sum += numbers[i];
    }
    printf("Sum of array elements: %d\n", sum);

    return 0;
}
